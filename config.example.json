{
  "servers": {
    "google-search": {
      "url": "http://localhost:3000",
      "env": {
        "GOOGLE_APPLICATION_CREDENTIALS": "${GOOGLE_CREDENTIALS_PATH}",
        "MCP_REGION": "${MCP_REGION}",
        "MCP_LOG_LEVEL": "${LOG_LEVEL}"
      }
    },
    "github-mcp": {
      "url": "http://localhost:3000",
      "env": {
        "GITHUB_TOKEN": "${GITHUB_TOKEN}",
        "MCP_REGION": "${MCP_REGION}",
        "MCP_LOG_LEVEL": "${LOG_LEVEL}"
      }
    }
  }
}